loading-text-opacity()
  @-moz-keyframes loading-text-opacity
    0%
      opacity 0
    20%
      opacity 0
    50%
      opacity 1
    100%
      opacity 0

  @-o-keyframes loading-text-opacity
    0%
      opacity 0
    20%
      opacity 0
    50%
      opacity 1
    100%
      opacity 0

  @-webkit-keyframes loading-text-opacity
    0%
      opacity 0
    20%
      opacity 0
    50%
      opacity 1
    100%
      opacity 0